	<typeAliases>
		<typeAlias type="com.atguigu.p2p.vo.CustomerModel" alias="CM"/>
		<typeAlias type="com.atguigu.p2p.vo.CustomerQueryModel" alias="CQM"/>
	</typeAliases>
	
	
	<plugins>
